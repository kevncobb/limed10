use Drupal\Core\Form\FormStateInterface;

/**
 * Implements hook_form_FORM_ID_alter().
 */
function limestone_landing_form_node_landing_page_edit_form_alter(&$form, FormStateInterface $form_state, $form_id) {
  // Add the Varbase Bootstrap Paragraphs default admin styling.
  $form['#attached']['library'][] = 'limestone_foundation_paragraphs/vbp-default-admin';
}

/**
 * Implements hook_form_FORM_ID_alter().
 */
function limestone_landing_form_node_landing_page_form_alter(&$form, FormStateInterface $form_state, $form_id) {
  // Add the Varbase Bootstrap Paragraphs default admin styling.
  $form['#attached']['library'][] = 'limestone_foundation_paragraphs/vbp-default-admin';
}
